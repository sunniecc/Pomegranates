.order-bug-container .bug-address {
  padding: 20rpx 30rpx;
}
.order-bug-container .bug-commodity-detail {
  margin-left: 30rpx;
  padding: 40rpx 30rpx 40rpx 0;
  border-bottom: 1px solid #eeeeee;
}
.order-bug-container .freight-price {
  margin-left: 30rpx;
  padding: 30rpx 30rpx 30rpx 0;
  border-bottom: 1px solid #eeeeee;
  font-weight: 200;
  font-size: small;
}
.order-bug-container .freight-price .right {
  float: right;
}
.order-bug-container .total-price {
  margin-left: 30rpx;
  padding: 30rpx 30rpx 30rpx 0;
  border-bottom: 1px solid #eeeeee;
  font-weight: 200;
  font-size: small;
}
.order-bug-container .total-price .right {
  float: right;
  font-weight: bold;
  font-size: large;
}
.order-bug-container .bug-tips {
  padding: 40rpx 30rpx;
  font-size: small;
  font-weight: 200;
}
.order-bug-container .bug-tips .down {
  margin-top: 20rpx;
}
.order-bug-container .bug-agree {
  margin: -10rpx 30rpx 30rpx;
  font-size: small;
  font-weight: 200;
}
.order-bug-container .bug-agree .tip {
  color: #01c2c3;
}
.order-bug-container .bug-fix-bottom {
  z-index: 100;
  position: fixed;
  bottom: 0;
  width: 100%;
  height: 120rpx;
  background-color: white;
  display: flex;
  align-items: center;
  justify-content: center;
}
.order-bug-container .bug-fix-bottom .bottom-price {
  width: 40%;
  font-weight: bold;
  color: #cc6666;
}
.order-bug-container .bug-fix-bottom .button {
  width: 40%;
}
.order-bug-container .bug-fix-bottom .button button {
  border-radius: 0;
  width: 100%;
  height: 70rpx;
  background-color: #01c2c3;
  color: white;
  font-size: large;
  font-size: bold;
  vertical-align: center;
  display: flex;
  align-items: center;
  justify-content: center;
}
